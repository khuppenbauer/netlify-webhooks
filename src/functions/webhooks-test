const messages = require('./methods/messages');

exports.handler = async (event) => {
  console.log(event);
  if (event.httpMethod === 'POST') {
    return messages.create(event, { foreignKey: 'test', app: 'test', event: 'test' });
  }
  return {
    statusCode: 405,
    body: 'Method Not Allowed',
  };
};
